{
  "name": "Pixiv Tag Visualizer",
  "description": "Visualizing author-defined tags on Pixiv",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["tag/tag.js"],
      "css": ["tag/tag.css"],
      "matches": ["https://www.pixiv.net/artworks/*"],
      "run_at": "document_start"
    }
  ]
}
